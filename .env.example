# Knowledge System Environment Configuration

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/knowledge

# Qdrant Vector Database
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION=knowledge

# Memgraph
MEMGRAPH_URL=bolt://localhost:7687

# Telegram Bot (for reports)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Evolution Bot (for approval system)
EVOLUTION_BOT_TOKEN=your_evolution_bot_token_here

# OpenAI (for Deep Learning)
OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face (for embeddings)
HUGGINGFACE_TOKEN=your_hf_token_here

# OpenClaw Gateway (for Deep Learning LLM requests)
OPENCLAW_GATEWAY_URL=http://localhost:18789
OPENCLAW_GATEWAY_TOKEN=your_openclaw_token_here

# Logging
LOG_LEVEL=INFO
LOG_PATH=/var/log/knowledge
