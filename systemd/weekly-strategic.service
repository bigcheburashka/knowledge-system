[Unit]
Description=Weekly Strategic Knowledge Extraction
After=network.target

[Service]
Type=oneshot
User=root
WorkingDirectory=/root/.openclaw/workspace/knowledge-system
EnvironmentFile=/etc/systemd/system/evolution-bot.env
ExecStart=/root/.openclaw/workspace/knowledge-system/scripts/weekly-strategic.sh
StandardOutput=append:/var/log/knowledge/weekly-strategic.log
StandardError=append:/var/log/knowledge/weekly-strategic.log

[Install]
WantedBy=multi-user.target
